        <file src="css/fixedHeader.jqueryui.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/fixedHeader.jqueryui.min.css" target="content\Content"/>
        <file src="css/fixedHeader.jqueryui.min.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="types/fixedHeader.jQueryUI.d.ts" target="content\Scripts"/>
        <file src="types/fixedHeader.jQueryUI.d.ts" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>

